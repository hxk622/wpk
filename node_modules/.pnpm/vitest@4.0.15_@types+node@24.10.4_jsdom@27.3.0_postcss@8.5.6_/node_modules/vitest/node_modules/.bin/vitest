#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/vitest@4.0.15_@types+node@24.10.4_jsdom@27.3.0_postcss@8.5.6_/node_modules/vitest/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/vitest@4.0.15_@types+node@24.10.4_jsdom@27.3.0_postcss@8.5.6_/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/vitest@4.0.15_@types+node@24.10.4_jsdom@27.3.0_postcss@8.5.6_/node_modules/vitest/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/vitest@4.0.15_@types+node@24.10.4_jsdom@27.3.0_postcss@8.5.6_/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
