#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules/z-schema/bin/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules/z-schema/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules:/Users/xingkaihan/Documents/Code/node_modules:/Users/xingkaihan/Documents/node_modules:/Users/xingkaihan/node_modules:/Users/node_modules:/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules/z-schema/bin/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules/z-schema/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/backend/node_modules:/Users/xingkaihan/Documents/Code/wpk/packages/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules:/Users/xingkaihan/Documents/Code/node_modules:/Users/xingkaihan/Documents/node_modules:/Users/xingkaihan/node_modules:/Users/node_modules:/node_modules:/Users/xingkaihan/Documents/Code/wpk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../z-schema/bin/z-schema" "$@"
else
  exec node  "$basedir/../z-schema/bin/z-schema" "$@"
fi
